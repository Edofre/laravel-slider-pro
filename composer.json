{
  "name": "edofre/laravel-slider-pro",
  "description": "Laravel widget for slider-pro",
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.3.*",
    "bower-asset/slider-pro": "1.3.0"
  },
  "keywords": [
    "laravel",
    "slider-pro",
    "jquery",
    "images",
    "slider"
  ],
  "homepage": "https://github.com/edofre/laravel-slider-pro",
  "license": "MIT",
  "support": {
    "source": "https://github.com/edofre/laravel-slider-pro",
    "issues": "https://github.com/edofre/laravel-slider-pro/issues"
  },
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Edofre\\SliderPro\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan vendor:publish --tag=slider-/pro"
    ],
    "post-update-cmd": [
      "php artisan vendor:publish --tag=slider-pro"
    ]
  }
}