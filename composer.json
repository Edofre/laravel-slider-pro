{
  "name": "edofre/laravel-slider-pro",
  "description": "Laravel widget for slider-pro",
  "keywords": [
    "laravel",
    "slider-pro",
    "jquery",
    "images",
    "slider"
  ],
  "homepage": "https://github.com/edofre/laravel-slider-pro",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.*",
    "npm-asset/slider-pro": "1.3.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.2.0|~3.3.0",
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Edofre\\SliderPro\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Edofre\\SliderPro\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Edofre\\SliderPro\\SliderProServiceProvider"
      ],
      "aliases": {
        "SliderPro": "Edofre\\SliderPro\\Facades\\SliderPro"
      }
    }
  }
}